#!/bin/sh
###########################################################################
# @(#)seq	1.1 16/06/23 Written 2016 by J. Schilling
###########################################################################
#@@C@@
###########################################################################
#
# Define a simple seq if the command is missing
#
###########################################################################
if type seq > /dev/null; then
	:
else
	seq() {
		cnt_seq=$1
		while [ $cnt_seq -le $2 ]; do
			echo $cnt_seq
			cnt_seq=`expr $cnt_seq + 1`
		done
	}
fi
