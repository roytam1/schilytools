#include <schily/mconfig.h>

SCHILY_1.0 {
global:
	getopt;	# getopt()

	optind;	# int optind
	opterr;	# int opterr
	optopt;	# int optopt
	optarg;	# char *optarg
	opt_sp; # int opt_sp	Not present with outdated getopt() on Solaris

#if defined(HAVE_PRAGMA_WEAK) && defined(HAVE_LINK_WEAK)
	_sp;	# int _sp	Compatibility to old SunOS programs
#endif

local:
	*;
};
